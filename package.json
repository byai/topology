{
  "name": "@byai/topology",
  "version": "1.3.18",
  "description": "拓扑图组件",
  "main": "dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/byai/topology"
  },
  "bugs": {
    "url": "https://github.com/byai/topology/issues"
  },
  "license": "MIT",
  "typings": "dist/lib/index.d.ts",
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -c config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack -c config/webpack.prod.js",
    "analy": "cross-env NODE_ENV=production webpack -c config/webpack.analy.js"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dnd": ">=2.5.4",
    "react-dnd-html5-backend": ">=2.5.4",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "d3-path": "^2.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "fs-extra": "7.0.0",
    "html2canvas": "^1.4.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.0.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.0.0",
    "lodash": "^4.17.11",
    "polished": "^2.3.3",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "resolve": "1.8.1",
    "scroll-into-view-if-needed": "^3.0.10",
    "shallow": "^1.0.0",
    "sinon": "^7.3.2",
    "snapshot-diff": "^0.5.1",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@svgr/webpack": "2.4.1",
    "@testing-library/react": "^8.0.1",
    "@types/invariant": "^2.2.35",
    "@types/jest": "23.3.12",
    "@types/lodash": "^4.14.121",
    "@types/node": "10.12.18",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6",
    "@types/styled-components": "^4.1.5",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.23.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "glob-all": "^3.3.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "react-refresh": "^0.14.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "thread-loader": "^3.0.4",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1",
    "webpack-merge": "^5.8.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^1.3.1",
    "jest-dom": "^3.5.0",
    "precss": "^4.0.0",
    "react": "^16.8.6",
    "react-dnd-test-backend": "^8.0.3",
    "react-dnd-test-utils": "^8.0.3",
    "react-dom": "^16.8.6",
    "standard-version": "^9.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "./transform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(react-dnd-test-utils|react-dnd-test-backend|react-dnd|dnd-core)).+\\.js$",
      "^.+\\.module\\.(css|sass|scss|less)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "16.8.6"
  }
}
