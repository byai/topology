{
  "name": "@byai/topology",
  "version": "1.13.0",
  "description": "拓扑图组件",
  "bugs": {
    "url": "https://github.com/byai/topology/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/byai/topology"
  },
  "license": "MIT",
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "build": "node remove-dist.js && tsc && gulp",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "eslint src --ext .tsx,.ts ",
    "lint-fix": "eslint --fix src --ext .tsx,.ts ",
    "precommit": "",
    "release": "./scripts/release.sh",
    "start": "node scripts/start.js",
    "test": "jest --no-cache"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react/cleanup-after-each"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "./transform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(react-dnd-test-utils|react-dnd-test-backend|react-dnd|dnd-core)).+\\.js$",
      "^.+\\.module\\.(css|sass|scss|less)$"
    ]
  },
  "dependencies": {
    "bezier-intersect": "^0.0.3",
    "classnames": "^2.2.6",
    "d3-path": "^2.0.0",
    "dagre": "^0.8.5",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.11",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "scroll-into-view-if-needed": "^3.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@babel/helper-string-parser": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@svgr/webpack": "4.3.3",
    "@testing-library/dom": "^5.4.0",
    "@testing-library/react": "^8.0.1",
    "@types/classnames": "^2.2.7",
    "@types/invariant": "^2.2.35",
    "@types/jest": "23.3.12",
    "@types/lodash": "^4.14.121",
    "@types/node": "10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "bfj": "6.1.1",
    "cache-loader": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "^2.4.2",
    "commitizen": "^4.2.4",
    "css-loader": "1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^4.0.1",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^1.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.0.0",
    "jest-dom": "^3.5.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^24.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-less": "^3.1.3",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "precss": "^4.0.0",
    "react": "^16.8.4",
    "react-app-polyfill": "^0.2.0",
    "react-dev-utils": "^11.0.4",
    "react-dnd-test-backend": "^8.0.3",
    "react-dnd-test-utils": "^8.0.3",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.8.6",
    "resolve": "1.8.1",
    "snapshot-diff": "^0.5.1",
    "standard-version": "^9.3.2",
    "style-loader": "0.23.0",
    "styled-components": "^4.1.3",
    "terser-webpack-plugin": "1.1.0",
    "thread-loader": "^2.1.1",
    "ts-import-plugin": "^1.5.5",
    "typescript": "^4.6.3",
    "url-loader": "1.1.1",
    "webpack": "4.39.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "4.7.3",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dnd": ">=2.5.4",
    "react-dnd-html5-backend": ">=2.5.4",
    "react-dom": ">=16.0.0"
  }
}
